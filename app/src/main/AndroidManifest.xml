        android:icon="@mipmap/whale_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/whale_app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Whale">
        <activity android:name=".addperson"></activity>
        <activity android:name=".ProfileInfoActivity" />
        <activity android:name=".LeaderActivity" />
        <activity android:name=".termandregulations" />
        <activity android:name=".ActivityJoinMembership" />
        <activity android:name=".FollowerActivity" />
